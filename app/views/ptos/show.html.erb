<div class="page-header">
  <h3>Pto #<%= @pto.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Image</dt>
      <dd><%= @pto.image %></dd>

      <dt>Caption</dt>
      <dd><%= @pto.caption %></dd>

      <dt>Owner</dt>
      <dd><%= @pto.owner_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/ptos" class="btn btn-primary">
        Back
      </a>
      <a href="/ptos/<%= @pto.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_pto/<%= @pto.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
