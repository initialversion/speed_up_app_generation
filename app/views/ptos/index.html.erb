<div class="page-header">
  <h1>
    Ptos
    <a href="/ptos/new" class="btn btn-lg btn-success">New Pto</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Image</th>
        <th>Caption</th>
        <th>Owner</th>
        <th>Actions</th>
      </tr>

      <% @ptos.each do |pto| %>
      <tr>
        <td><%= pto.image %></td>
        <td><%= pto.caption %></td>
        <td><%= pto.owner_id %></td>
        <td>
          <a href="/ptos/<%= pto.id %>" class="btn btn-primary">Show</a>
          <a href="/ptos/<%= pto.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_pto/<%= pto.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
